POSTGRES_USER=your_user
POSTGRES_PASSWORD=your_password
POSTGRES_DB=kgaps
# Copy this file to db.env and fill in real credentials for development

# Flask environment (development or production)
FLASK_ENV=development

# CORS Configuration - Add your device IPs/origins here (comma-separated)
# In development mode with FLASK_ENV=development, all origins are allowed
# In production, specify allowed origins explicitly
# Example: http://localhost:3000,http://192.168.1.100:3000,http://10.0.0.5:3000
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
